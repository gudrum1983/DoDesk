{
  "name": "dodesk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --mode development\" \"yarn server\"",
    "dev:web": "vite --mode web",
    "server": "json-server -w db.json -p 3001",
    "build": "tsc -b && vite build",
    "build:local": "vite build --mode development",
    "build:web": "vite build --mode web",
    "preview:local": "yarn build:local && concurrently \"vite preview --mode development\" \"yarn server\"",
    "preview:web": "yarn build:web && vite preview",
    "lint": "eslint .",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.3.0",
    "cors": "^2.8.5",
    "json-serve": "^0.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.1.2",
    "react-router-dom": "6",
    "react-window": "^1.8.10",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@types/cors": "^2",
    "@types/gh-pages": "^6",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-window": "^1",
    "@vitejs/plugin-react": "4.3.3",
    "concurrently": "9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "9.13.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "gh-pages": "^6.2.0",
    "globals": "15.11.0",
    "json-server": "^1.0.0-beta.3",
    "sass": "1.80.5",
    "typescript": "5.6.2",
    "typescript-eslint": "8.12.2",
    "vite": "5.4.10"
  },
  "packageManager": "yarn@4.5.1"
}
